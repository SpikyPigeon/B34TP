{
  "extends": "../tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "downlevelIteration": true,
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types"
    ],
